<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head>
    	<w2:type>DEFAULT</w2:type>
        <w2:buildDate/>
        <xf:model>
        	<xf:instance>
        		<data xmlns="" />
        	</xf:instance>
        	<w2:dataCollection baseNode="map">
        		<w2:dataList baseNode="list" repeatNode="map"
        			id="dc_qnaPostList" saveRemovedData="true">
        			<w2:columnInfo>
        				<w2:column id="RNUM" name="번호"
        					dataType="number">
        				</w2:column>
        				<w2:column id="SBR_UUID" name="게시글번호"
        					dataType="text">
        				</w2:column>
        				<w2:column id="SBR_TITLE" name="게시글제목"
        					dataType="text">
        				</w2:column>
        				<w2:column id="MEM_NAME" name="작성자"
        					dataType="text">
        				</w2:column>
        				<w2:column id="SBR_REGDATE" name="작성일"
        					dataType="text">
        				</w2:column>
        			</w2:columnInfo>
        		</w2:dataList>
        		<w2:dataMap baseNode="map" id="dc_qnaSearch">
        			<w2:keyInfo>
        				<w2:key id="SBR_TITLE" name="게시글제목"
        					dataType="text">
        				</w2:key>
        			</w2:keyInfo>
        		</w2:dataMap>
        	</w2:dataCollection>
        	<w2:workflowCollection></w2:workflowCollection>
        	<xf:submission id="sub_PostListSelect"
        		ref="data:json,dc_qnaSearch" target="data:json,dc_qnaPostList"
        		action="/sysQnaMgr.do" method="post"
        		mediatype="application/json" encoding="UTF-8" instance=""
        		replace="" errorHandler="" customHandler="" mode="synchronous"
        		processMsg="데이터로딩중..." ev:submit="" ev:submitdone=""
        		ev:submiterror="">
        	</xf:submission>
        </xf:model>
        <script type="text/javascript"><![CDATA[
        scwin.init() = function(){
        	$w.executeSubmission( "sub_PostListSelect" );
        };]]></script>
    </head>
    <body>
    	<xf:group style="padding : 10px;" id="" class="">
    		<w2:textbox label="관리자 - 문의 게시판"
    			style="margin-bottom : 10px;text-align: center;" id=""
    			tagname="h3">
    		</w2:textbox><xf:group style="text-align: right;margin-bottom : 10px;" id="" class="">
    			<xf:select1 selectedData="true" submenuSize="auto"
    				allOption="" optionOrder="false" disabled="false" id=""
    				direction="auto" chooseOption="" displayMode="label"
    				appearance="minimal" disabledClass="w2selectbox_disabled"
    				style="width: 150px;height: 25px;">
    				<xf:choices>
    					<xf:item>
    						<xf:label><![CDATA[5]]></xf:label>
    						<xf:value><![CDATA[5]]></xf:value>
    					</xf:item>
    					<xf:item>
    						<xf:label><![CDATA[10]]></xf:label>
    						<xf:value><![CDATA[10]]></xf:value>
    					</xf:item>
    					<xf:item>
    						<xf:label><![CDATA[15]]></xf:label>
    						<xf:value><![CDATA[15]]></xf:value>
    					</xf:item>
    					<xf:item>
    						<xf:label><![CDATA[20]]></xf:label>
    						<xf:value><![CDATA[20]]></xf:value>
    					</xf:item>
    				</xf:choices>
    			</xf:select1>
</xf:group><xf:group style="text-align: left;margin-bottom : 10px;" id="" class="">
    			<xf:input id="" style="width: 200px;height: 25px;margin-right : 10px;" ref="data:dc_qnaSearch.SBR_TITLE">
    			</xf:input>
    			<xf:trigger type="button" style="width: 80px;height: 23px;" id="">
    				<xf:label><![CDATA[검색]]></xf:label>
    			</xf:trigger>
    		</xf:group>
    		
    		<w2:gridView id="" ignoreToggleOnDisabled="false"
    			useShiftKey="true" style="margin-bottom : 10px;"
    			useFilterList="false" scrollByColumn="false"
    			scrollByColumnAdaptive="false" summaryAuto="false"
    			fixedColumnWithHidden="true" summaryOnlyAuto="false"
    			dataList="dc_qnaPostList" ignoreCellClick="false">
    			<w2:caption style="" id="caption1"
    				value="this is a grid caption.">
    			</w2:caption>
    			<w2:header style="" id="header1">
    				<w2:row style="" id="row1">
    					<w2:column width="10%" inputType="text"
    						style="height:20px" id="column9" blockSelect="false"
    						displayMode="label" value="번호">
    					</w2:column>
    					<w2:column width="50%" inputType="text"
    						style="height:20px" id="column5" blockSelect="false"
    						displayMode="label" value="게시글제목">
    					</w2:column>
    					<w2:column width="20%" inputType="text"
    						style="height:20px" id="column3" blockSelect="false"
    						displayMode="label" value="작성자">
    					</w2:column>
    					<w2:column width="20%" inputType="text"
    						style="height:20px" id="column1" blockSelect="false"
    						displayMode="label" value="작성일">
    					</w2:column>
    				</w2:row>
    			</w2:header>
    			<w2:gBody style="" id="gBody1">
    				<w2:row style="" id="row2">
    					<w2:column width="10%" inputType="text"
    						style="height:20px" id="RNUM" blockSelect="false"
    						displayMode="label">
    					</w2:column>
    					<w2:column width="50%" inputType="text"
    						style="height:20px" id="SBR_TITLE" blockSelect="false"
    						displayMode="label">
    					</w2:column>
    					<w2:column width="20%" inputType="text"
    						style="height:20px" id="MEM_NAME" blockSelect="false"
    						displayMode="label">
    					</w2:column>
    					<w2:column width="20%" inputType="text"
    						style="height:20px" id="SBR_REGDATE" blockSelect="false"
    						displayMode="label">
    					</w2:column>
    				</w2:row>
    			</w2:gBody>
    		</w2:gridView>



    	</xf:group>
